<p id="notice"><%= notice %></p>

<h1>Collaborations</h1>

<table>
  <thead>
    <tr>
      <th>Iduser</th>
      <th>Idimdb</th>
      <th>Idspotify</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collaborations.each do |collaboration| %>
      <tr>
        <td><%= collaboration.idUser %></td>
        <td><%= collaboration.idImdb %></td>
        <td><%= collaboration.idSpotify %></td>
        <td><%= collaboration.state %></td>
        <td><%= link_to 'Show', collaboration %></td>
        <td><%= link_to 'Edit', edit_collaboration_path(collaboration) %></td>
        <td><%= link_to 'Destroy', collaboration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collaboration', new_collaboration_path %>
